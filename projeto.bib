@article{romero-lankao:2016urban8,
  title = {Urban {{Sustainability}} and {{Resilience}}: {{From Theory}} to {{Practice}}},
  shorttitle = {Urban {{Sustainability}} and {{Resilience}}},
  author = {Romero-Lankao, Patricia and Gnatz, Daniel M. and Wilhelmi, Olga and Hayden, Mary},
