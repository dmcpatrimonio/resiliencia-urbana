@article{gurgel:2017aliando6,
  title = {Aliando teoria e prática: um exame da metodologia de “Análise Arquitetônica” aplicada em disciplinas de Teoria e História de Arquitetura},
  shorttitle = {Aliando teoria e prática},
  author = {Gurgel, Ana Paula Campos},
