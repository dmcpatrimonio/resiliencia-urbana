@article{wang:2020life13,
  title = {Life {{Cycle Environmental Costs}} of {{Buildings}}},
  author = {Wang, Yuanfeng and Pang, Bo and Zhang, Xiangjie and Wang, Jingjing and Liu, Yinshan and Shi, Chengcheng and Zhou, Shuowen},
  date = {2020-01},
  journaltitle = {Energies},
